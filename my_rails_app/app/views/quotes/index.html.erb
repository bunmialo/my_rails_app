<h1>Listing All Bunmi's Quotes</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th colspan="2"></th>
  </tr>

  <% @quotes.each do |quote| %>
    <tr>
      <td><%= quote.title %></td>
      <td><%= quote.text %></td>
      <td><%= link_to 'Show', quote_path(quote) %></td>
      <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
    </tr>
  <% end %>
</table>